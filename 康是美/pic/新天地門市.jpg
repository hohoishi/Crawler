    <script>
        var catData = "";
        $(document).ready(function () {

            $.ajax({
                url: "/api/getCategory.aspx",
                success: function (resp) {

                    if (resp.result == "OK") {
                        r = resp.data;
                        catData = r;
                        var s = Math.ceil((r.length / 3.0));
                        s = 9;
                        var h = "";
                        var h2 = "";
                        var c = 0;
                        for (i = 0; i < 3; i++) {
                            h += '<ul class="submenu_l2" id="menuColumn_' + (i + 1) + '">\n';
                        
                            for (j = 0; j < s; j++) {
                                if (i * s + j < r.length) {
                                    var menu = r[i * s + j];
                                    if (menu.ChildList.length > 0) {
                                        h += '<li class="hassubmenu"><a href="#here" menuid="' + (i*s+j) + '" >' + menu.Name + '</a></li>\n';
                                    } else {
                                        h += '<li ><a href="http://shop.cosmed.com.tw/v2/official/SalePageCategory/' + menu.id + '" target="_blank">' + menu.Name + '</a></li>\n';
                                    }
                                }
                            }
                            h += "</ul>\n"
                        
                        }
                        $("#menu_lv1").html(h);

                        for (i = 0; i < r.length; i++) {
                            menu = r[i];
                            if (menu.ChildList.length > 0) {
                                h2 += '<li class="hassubmenu"><a href="#here"  >' + menu.Name + '</a>';
                                h2 += '<ul class="mainmenu_dropmenu_l3">';
                                for (j = 0; j < menu.ChildList.length; j++) {
                                    h2 += '<li ><a href="http://shop.cosmed.com.tw/v2/official/SalePageCategory/' + menu.ChildList[j].id + '">' + menu.ChildList[j].Name + '</a></li>\n';
                                }
                                h2 += "</ul>";
                                h2 += "</li>\n"
                            } else {
                                h2 += '<li ><a href="http://shop.cosmed.com.tw/v2/official/SalePageCategory/' + menu.id + '">' + menu.Name + '</a></li>\n';
                            }
                        }
                        $(".mainmenu_dropmenu-block li:nth-child(2) .mainmenu_dropmenu_l2").html(h2);

                        $(".mainmenu_dropmenu_l2").hide();
                        bindMenuEvent();
                    }
                }
            });


        });


    </script>


